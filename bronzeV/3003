import java.util.*;
import java.io.*;
import java.math.*;

public class Main {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);

		int[] a= {1,1,2,2,2,8};
		int[] b= new int[6];
		int[] c= new int[6];
		for(int i=0;i<6;i++)
		{
			b[i]=scanner.nextInt();
		}
		for(int i=0;i<6;i++)
		{
			if(a[i]!=b[i])
			{
				c[i]=a[i]-b[i];
			}
		}
		for(int i=0;i<6;i++)
		{
			System.out.print(c[i]+" ");
		}
			
		
	}
}

