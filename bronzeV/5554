import java.util.*;
import java.io.*;
import java.math.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int sum=0;
		for(int i=0;i<4;i++)
		{
			int a=sc.nextInt();
			sum+=a;
		}
		System.out.println(sum/60);
		System.out.println(sum%60);
	}
}
