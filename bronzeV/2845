import java.util.*;
import java.io.*;
import java.math.*;

public class Main {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);

		int a=scanner.nextInt();
		int b=scanner.nextInt();
		int sum=0;
		for(int i=0;i<5;i++)
		{
			int c=scanner.nextInt();
			sum = c-(a*b);
			System.out.print(sum+" ");
		}
		
	}
}
