import java.util.*;
import java.io.*;
import java.math.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		int c=b;
		int d=0;
		for(int i=0;i<a;i++)
		{
			c+=sc.nextInt();
			d+=sc.nextInt();
		}
		System.out.println("비와이");
	}
}
