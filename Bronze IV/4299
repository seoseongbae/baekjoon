import java.util.*;
import java.io.*;
import java.math.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b=sc.nextInt();
		int x=(a+b)/2;
		int y=(a-b)/2;
		if(a<b)
		{
			System.out.println(-1);
		}
		else {
			if(x+y==a&&x-y==b)
			{
				System.out.println(x+" "+y);
			}
			else
			{
				System.out.println(-1);
			}
			
		}

	}

}
