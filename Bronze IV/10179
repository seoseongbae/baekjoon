import java.util.*;
import java.io.*;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int a=sc.nextInt();
		double[] arr=new double[a];
		for(int i=0;i<arr.length;i++)
		{
			arr[i]=sc.nextDouble();
			arr[i]=arr[i]*0.8;
		}
		
		
		
		for(int i=0;i<arr.length;i++)
		{
			System.out.printf("$%.2f\n",arr[i]);
		}
	}
}

