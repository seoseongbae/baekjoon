import java.util.*;
import java.io.*;
import java.math.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int d = sc.nextInt();
		int h = sc.nextInt();
		int w = sc.nextInt();
		
		double a = Math.pow(d,2)/(Math.pow(h,2)+Math.pow(w, 2));
		System.out.println((int)Math.sqrt(a*Math.pow(h, 2))+" "+(int)Math.sqrt(a*Math.pow(w, 2)) );
	}
}
